{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.inlayHints.enabled": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "javascript.validate.enable": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "scss"],
  "cSpell.userWords": [
    "actuals",
    "Arnaud",
    "combobox",
    "compset",
    "cond",
    "defintion",
    "Droppable",
    "financials",
    "hypernova",
    "kanban",
    "labelledby",
    "listbox",
    "Memoization",
    "monospaced",
    "nullish",
    "Onboarding",
    "organisation",
    "prefetch",
    "prefilled",
    "refetch",
    "refetched",
    "REPRIORITIZED",
    "scrollable",
    "sideload",
    "sideloaded",
    "stylelint",
    "submarket",
    "teamleader",
    "Timesheets",
    "timetracking",
    "timetrackings",
    "Tooltipped",
    "unmount",
    "Upsell",
    "usersnap",
    "Weyts"
  ],
  "conventionalCommits.gitmoji": false,
  "conventionalCommits.promptFooter": true,
  "conventionalCommits.promptScopes": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.maxComputationTime": 0,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.lineHeight": 26,
  "editor.multiCursorModifier": "alt",
  "editor.stickyScroll.enabled": true,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact",
    "postcss": "css"
  },
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.js": "javascript",
    "*.jsx": "javascriptreact",
    "*.snap": "javascriptreact",
    "*.tsx": "typescriptreact"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "gitlens.remotes": [
    {
      "domain": "git.otainsight.com",
      "type": "GitLab"
    }
  ],
  "gitlens.views.commits.files.layout": "tree",
  "gitlens.views.stashes.files.layout": "list",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "scm.defaultViewMode": "tree",
  "task.problemMatchers.neverPrompt": {
    "npm": true
  },
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.fontFamily": "monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.scrollback": 10000,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "window.newWindowDimensions": "maximized",
  "window.restoreFullscreen": true,
  "workbench.colorTheme": "One Monokai",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 7,
  "workbench.editor.wrapTabs": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "readme",
  "gitlens.autolinks": [
    {
      "url": null,
      "prefix": null,
      "ignoreCase": false,
      "alphanumeric": false
    }
  ],
  "editor.inlineSuggest.enabled": true,
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#dd0531"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#61dafb"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Svelte Orange",
      "value": "#ff3d00"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "window.experimental.useSandbox": false,
  "workbench.layoutControl.enabled": false,
  "python.terminal.activateEnvironment": false,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.views.branches.branches.layout": "list",
  "[python]": {
    "editor.formatOnType": true
  },
  "workbench.activityBar.visible": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "gitlens.graph.dimMergeCommits": true
}
